Cell[TextData[{
 "The zeroth order equation of motion for the scalar field is derived from \
the Lagrangian ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["L", "TI"], "\[LongEqual]", 
         SqrtBox[
          RowBox[{"-", 
            StyleBox["g", "TI"]}]], 
         RowBox[{"(", 
           RowBox[{"-", 
             FractionBox[
              RowBox[{
                SuperscriptBox["\[Del]", "\[Mu]"], "\[Phi]", 
                SubscriptBox["\[Del]", "\[Mu]"], "\[Phi]"}], "2"], "-", 
             StyleBox["V", "TI"], 
             RowBox[{"(", "\[Phi]", ")"}], "-", 
             SubscriptBox["\[Rho]", "dm"]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$L = \\sqrt {-g}\\left(-\\frac{\\nabla^\\mu\\phi \\nabla_\\mu\\phi}{2} \
- V\\left(\\phi \\right) - \\rho_\\textnormal{\\gls {dm}}\\right)$", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "9be3bfc5-68ab-4a3a-a663-f9e71c6ec72f"],
 " by computing ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"0", "\[LongEqual]", 
         FractionBox[
          RowBox[{"\[Delta]", 
            StyleBox["L", "TI"]}], 
          RowBox[{"\[Delta]\[Phi]", "(", 
            RowBox[{
              StyleBox["t", "TI"], ",", 
              StyleBox["x", "TI"], ",", 
              StyleBox["y", "TI"], ",", 
              StyleBox["z", "TI"]}], ")"}]], "-", 
         SubscriptBox["\[PartialD]", "\[Mu]"], 
         FractionBox[
          RowBox[{"\[Delta]", 
            StyleBox["L", "TI"]}], 
          RowBox[{"\[Delta]", "(", 
            RowBox[{
              SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]", 
              RowBox[{"(", 
                RowBox[{
                  StyleBox["t", "TI"], ",", 
                  StyleBox["x", "TI"], ",", 
                  StyleBox["y", "TI"], ",", 
                  StyleBox["z", "TI"]}], ")"}]}], ")"}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$0 = \\frac {\\delta L} {\\delta \\phi\\left (t, x, y, z \\right)} - \
\\partial_\\mu\\frac {\\delta L} {\\delta\\left (\\partial_\\mu\\phi\\left \
(t, x, y, z \\right)\\right)}$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "10ff0ce2-0f1f-4bbc-8204-626bb83d4e47"],
 " in ",
 Cell[BoxData[
  RowBox[{"xPert", ".", "nb"}]], "InlineCode",ExpressionUUID->
  "4fa1d49f-3f43-41d3-8400-41ede72645a1"],
 ". This yields the modified Klein\[Dash]Gordon equation ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"0", "\[LongEqual]", 
         SubscriptBox["\[Del]", "\[Mu]"], 
         SuperscriptBox["\[Del]", "\[Mu]"], "\[Phi]", "-", 
         SuperscriptBox[
          StyleBox["V", "TI"], "\[Prime]"], "-", 
         SubsuperscriptBox["\[Rho]", "dm", "\[Prime]"]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
