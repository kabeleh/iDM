Cell[TextData[{
 "We find ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Rho]", "\[Phi]"], "\[LongEqual]", 
         SubscriptBox["\[Rho]", "background"], 
