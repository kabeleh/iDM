  3.9737588565136127`*^9}},ExpressionUUID->"0bf6b29c-a243-4c4d-b0ae-\
06034e7c91df"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"potentials", "=", 
    RowBox[{"<|", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\"\<pl\>\"", "->", 
       RowBox[{
        RowBox[{"c", "^", 
         RowBox[{"(", 
          RowBox[{"4", "-", "p"}], ")"}]}], " ", 
        RowBox[{"\[Phi]", "^", "p"}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"\"\<cos\>\"", "->", 
       RowBox[{"c1", " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"c2", " ", "\[Phi]"}], "]"}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"\"\<hyp\>\"", "->", 
       RowBox[{"c1", " ", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"Tanh", "[", 
           RowBox[{"c2", " ", "\[Phi]"}], "]"}]}], ")"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\"\<pNG\>\"", "->", 
       RowBox[{
        RowBox[{"c1", "^", "4"}], " ", 
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"Cos", "[", 
           RowBox[{"\[Phi]", "/", "c2"}], "]"}]}], ")"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\"\<iPL\>\"", "->", 
       RowBox[{
        RowBox[{"c", "^", 
         RowBox[{"(", 
          RowBox[{"p", "+", "4"}], ")"}]}], " ", 
        RowBox[{"\[Phi]", "^", 
         RowBox[{"(", 
          RowBox[{"-", "p"}], ")"}]}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"\"\<exp\>\"", "->", 
       RowBox[{"c1", " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "c2"}], " ", "\[Phi]"}], "]"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\"\<sqexp\>\"", "->", 
       RowBox[{
        RowBox[{"c", "^", 
         RowBox[{"(", 
          RowBox[{"p", "+", "4"}], ")"}]}], " ", 
        RowBox[{"\[Phi]", "^", 
         RowBox[{"(", 
          RowBox[{"-", "p"}], ")"}]}], " ", 
        RowBox[{"Exp", "[", 
         RowBox[{"c", " ", 
          RowBox[{"\[Phi]", "^", "2"}]}], "]"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\"\<bexp\>\"", "->", 
       RowBox[{"c1", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"\[Phi]", "^", "2"}], "+", "c2"}], ")"}], " ", 
        RowBox[{"Exp", "[", 
         RowBox[{
          RowBox[{"-", "c3"}], " ", "\[Phi]"}], "]"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\"\<dexp\>\"", "->", 
       RowBox[{
        RowBox[{"c1", " ", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"-", "c2"}], " ", "\[Phi]"}], "]"}]}], "+", 
        RowBox[{"c3", " ", 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"-", "c4"}], " ", "\[Phi]"}], "]"}]}]}]}]}], "|>"}]}], 
   ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"vars", "=", 
