   RowBox[{"\[Rho]", " ", "3", " ", 
    RowBox[{
     RowBox[{"\[Gamma]", "/", "exactlambda"}], "/", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"3", " ", 
