Cell[BoxData[{
 RowBox[{
  RowBox[{"exactGamma", "=", 
   RowBox[{"ParallelTable", "[", 
    RowBox[{
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"V", "=", 
         RowBox[{"potentials", "[", 
          RowBox[{"[", "name", "]"}], "]"}]}], "}"}], ",", 
       RowBox[{"V", " ", 
        RowBox[{
         RowBox[{"D", "[", 
          RowBox[{"V", ",", 
           RowBox[{"{", 
            RowBox[{"\[Phi]", ",", "2"}], "}"}]}], "]"}], "/", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"D", "[", 
            RowBox[{"V", ",", "\[Phi]"}], "]"}], ")"}], "^", "2"}]}]}]}], 
      "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"name", ",", 
