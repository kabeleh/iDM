Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Compute", " ", "small"}], "-", 
    RowBox[{
    "phi", " ", "initial", " ", "conditions", " ", "in", " ", "parallel"}]}], 
   "*)"}], 
  RowBox[{
   RowBox[{
    RowBox[{"smallPhiInitial", "=", 
     RowBox[{"ParallelTable", "[", 
      RowBox[{
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"name", "=", 
            RowBox[{"names", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ",", 
           RowBox[{"V", "=", 
            RowBox[{"vals", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ",", 
           RowBox[{"LambdaSmallPhi", "=", 
            RowBox[{"lambdaLimitSmallPhi", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ",", "VSmallPhi", ",", "eqn", ",",
            "result"}], "}"}], ",", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{
           "Asymptotic", " ", "form", " ", "of", " ", "V", " ", "as", " ", 
            "\[Phi]"}], "\[RightArrow]", 
           RowBox[{"0", " ", 
            RowBox[{"(", 
             RowBox[{"leading", " ", "term", " ", "only"}], ")"}]}]}], "*)"}], 
         RowBox[{
          RowBox[{"VSmallPhi", "=", 
           RowBox[{"Normal", "@", 
            RowBox[{"Series", "[", 
             RowBox[{"V", ",", 
              RowBox[{"{", 
               RowBox[{"\[Phi]", ",", "0", ",", "1"}], "}"}]}], "]"}]}]}], ";",
           "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
           "Construct", " ", "attractor", " ", "equation", " ", "using", " ", 
            "asymptotic", " ", "Lambda", " ", "and", " ", "V"}], "*)"}], 
          RowBox[{"eqn", "=", 
           RowBox[{
            RowBox[{
             RowBox[{"2", " ", 
              RowBox[{"VSmallPhi", "/", 
               RowBox[{"(", 
                RowBox[{"1", "-", "\[Omega]"}], ")"}]}]}], "==", 
             RowBox[{"\[Rho]", " ", "3", " ", 
              RowBox[{
               RowBox[{"\[Gamma]", "/", 
                RowBox[{
                 RowBox[{"(", "LambdaSmallPhi", ")"}], "^", "2"}]}], "/", 
               RowBox[{"(", 
                RowBox[{"1", "-", 
                 RowBox[{"3", " ", 
                  RowBox[{"\[Gamma]", "/", 
                   RowBox[{
                    RowBox[{"(", "LambdaSmallPhi", ")"}], "^", "2"}]}]}]}], 
                ")"}]}]}]}], "&&", 
            RowBox[{"p", ">", "0"}], "&&", 
            RowBox[{"\[Gamma]", ">", "0"}], "&&", 
            RowBox[{"\[Rho]", ">", "0"}], "&&", 
            RowBox[{"\[Omega]", ">", "0"}]}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{
           "Try", " ", "to", " ", "solve", " ", "for", " ", "\[Phi]"}], 
           "*)"}], 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"name", "===", "\"\<iPL\>\""}], ",", 
            RowBox[{"(*", 
             RowBox[{"Use", " ", "Reduce", " ", "for", " ", "iPL"}], "*)"}], 
            RowBox[{"result", "=", 
             RowBox[{"Quiet", "@", 
              RowBox[{"Reduce", "[", 
               RowBox[{"eqn", ",", "\[Phi]", ",", "Reals"}], "]"}]}]}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"(*", 
             RowBox[{
             "Use", " ", "Solve", " ", "for", " ", "all", " ", "other", " ", 
              "potentials"}], "*)"}], 
            RowBox[{"Module", "[", 
             RowBox[{
              RowBox[{"{", "soln", "}"}], ",", 
              RowBox[{
               RowBox[{"soln", "=", 
                RowBox[{"Quiet", "@", 
                 RowBox[{"Solve", "[", 
                  RowBox[{"eqn", ",", "\[Phi]", ",", "Reals"}], "]"}]}]}], ";",
                "\[IndentingNewLine]", 
               RowBox[{"result", "=", 
                RowBox[{"If", "[", 
                 RowBox[{
                  RowBox[{"soln", "=!=", 
                   RowBox[{"{", "}"}]}], ",", 
                  RowBox[{"First", "[", "soln", "]"}], 
                  ",", "\"\<no real solution / \[Phi] unconstrained\>\""}], 
                 "]"}]}]}]}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"name", ",", "result"}], "}"}]}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "[", "vals", "]"}]}], "}"}]}], "]"}]}], ";"}], 
