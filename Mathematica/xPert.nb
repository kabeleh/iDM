Cell[TextData[{
 "The equations of motion are derived from the Lagrangian as ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"0", "\[LongEqual]", 
         FractionBox[
          RowBox[{"\[Delta]", 
            StyleBox["L", "TI"]}], 
          RowBox[{"\[Delta]\[Phi]", "(", 
            RowBox[{
              StyleBox["t", "TI"], ",", 
              StyleBox["x", "TI"], ",", 
              StyleBox["y", "TI"], ",", 
              StyleBox["z", "TI"]}], ")"}]], "-", 
         SubscriptBox["\[PartialD]", "\[Mu]"], 
         FractionBox[
          RowBox[{"\[Delta]", 
            StyleBox["L", "TI"]}], 
          RowBox[{"\[Delta]", "(", 
            RowBox[{
              SubscriptBox["\[PartialD]", "\[Mu]"], "\[Phi]", 
              RowBox[{"(", 
                RowBox[{
                  StyleBox["t", "TI"], ",", 
                  StyleBox["x", "TI"], ",", 
                  StyleBox["y", "TI"], ",", 
                  StyleBox["z", "TI"]}], ")"}]}], ")"}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$0=\\frac{\\delta L}{\\delta \
\\phi\\left(t,x,y,z\\right)}-\\partial_\\mu\\frac{\\delta \
L}{\\delta\\left(\\partial_\\mu\\phi\\left(t,x,y,z\\right)\\right)}$", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3d02f820-331e-40c8-bb2d-8571de1ce9bc"],
 "."
}], "Text",
 CellChangeTimes->{{3.9707268792928743`*^9, 
  3.9707269180031652`*^9}},ExpressionUUID->"cc25ae85-56c4-4163-b497-\
ebb37575837b"],

