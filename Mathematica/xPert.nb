 ", ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox["\[Delta]\[Phi]", 
          RowBox[{";", "\[Mu]"}], 
          RowBox[{";", "\[Mu]"}]], "\[LongEqual]", "\[EmptySquare]", 
         "\[Delta]\[Phi]"}], TraditionalForm], "errors" -> {}, "input" -> 
     "$\\delta\\phi^{;\\mu}_{;\\mu}=\\Box\\delta\\phi$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0f83c3cb-a8de-4803-86e6-625739530a13"],
