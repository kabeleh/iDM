 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["T", "TI"], "\[Mu]\[Nu]"], "\[LongEqual]", 
         FractionBox["2", 
          SqrtBox[
           RowBox[{"-", 
             StyleBox["g", "TI"]}]]], 
         FractionBox[
          RowBox[{"\[Delta]", "(", 
            RowBox[{
              SqrtBox[
               RowBox[{"-", 
                 StyleBox["g", "TI"]}]], 
              StyleBox["L", "TI"]}], ")"}], 
          RowBox[{"\[Delta]", 
            SubscriptBox[
             StyleBox["g", "TI"], "\[Mu]\[Nu]"]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$T^{\\mu \\nu}  = \\frac {2}{\\sqrt{-g}}\\frac \
{\\delta\\left(\\sqrt{-g} L \\right)}{\\delta g_ {\\mu \\nu}}$", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "11b9e821-7351-4e0b-a380-d812ff4734ca"]
}], "Text",
