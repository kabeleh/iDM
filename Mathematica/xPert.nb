Cell[TextData[{
 "We used the zeroth-order equation of motion (",
 Cell[BoxData[
  RowBox[{"eom0", "==", "0"}]], "InlineCode",ExpressionUUID->
  "a00849b0-2ae3-4f72-a7f5-af87b18c05e9"],
 ") to simplify the expression. The resulting first-order equation of motion \
is ",
 Cell[BoxData[
  RowBox[{"0", "\[LongEqual]", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", 
        SubsuperscriptBox["\[Rho]", "DM", "\[Prime]\[Prime]"]}], "-", 
       SuperscriptBox[
        StyleBox["V", "TI"], "\[Prime]\[Prime]"], "+", 
       TemplateBox[<|
        "boxes" -> FormBox["\[EmptySquare]", TraditionalForm], "errors" -> {},
          "input" -> "\\Box", "state" -> "Boxes"|>,
        "TeXAssistantTemplate"]}], ")"}], "\[Delta]\[Phi]"}], "-", 
    RowBox[{
     SuperscriptBox[
      StyleBox["m", "TI"], "\[Prime]"], "\[CenterDot]", 
     StyleBox[
      RowBox[{"\[Delta]", 
       StyleBox["n", "TI"]}]]}], "+", 
    RowBox[{
     SuperscriptBox["\[Phi]", 
      RowBox[{";", "\[Mu]"}]], "\[CenterDot]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox["1", "2"], 
        SubsuperscriptBox[
         StyleBox["h", "TI"], 
         RowBox[{"\[Nu]", ";", "\[Mu]"}], "\[Nu]"]}], "-", 
       SubsuperscriptBox[
        StyleBox["h", "TI"], 
        RowBox[{"\[Mu]", ";", "\[Nu]"}], "\[Nu]"]}], ")"}]}], "-", 
    RowBox[{
     SuperscriptBox[
      StyleBox["h", "TI"], "\[Mu]\[Nu]"], 
     SubscriptBox["\[Phi]", 
      RowBox[{";", "\[Nu]\[Mu]"}]]}]}]}]], "Input",ExpressionUUID->
  "27017b5d-706d-4130-9d70-51b7ffad2c30"],
 ", with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Delta]", 
         StyleBox["n", "TI"]}], TraditionalForm], "errors" -> {}, "input" -> 
     "$\\delta n$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "250b9434-c63b-4647-9a5d-b9712a381d42"],
 " the dark matter number density perturbation, ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox["\[Phi]", 
          RowBox[{";", "\[Mu]"}]], "\[LongEqual]", 
         SuperscriptBox["\[Del]", "\[Mu]"], "\[Phi]"}], TraditionalForm], 
     "errors" -> {}, "input" -> "$\\phi^{;\\mu}=\\nabla^\\mu\\phi$", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
